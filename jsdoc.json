{
	"tags" : {
		"allowUnknownTags" : true,
		"dictionaries" : [
			"jsdoc",
			"closure"
		]
	},
	"source" : {
		"include" : [
			"./src/"
		],
		"exclude" : [],
		"includePattern" : ".+\\.js(doc)?$",
		"excludePattern" : "(^|\\/|\\\\)_"
	},
	"opts" : {
		"encoding" : "utf8",
		"destination" : "./docs/",
		"recurse" : true,
		"verbose" : true
	}
}